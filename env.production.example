# Configurație pentru producție
NODE_ENV=production
VITE_API_URL=https://your-domain.com
VITE_APP_NAME=Time Management System
VITE_APP_VERSION=1.0.0

# Configurație backend pentru producție
DB_HOST=localhost
DB_USER=your_db_user
DB_PASSWORD=your_secure_password
DB_NAME=kpi_tracker_prod
DB_PORT=3306

# Configurație securitate
JWT_SECRET=your_super_secure_jwt_secret_key_here
CORS_ORIGINS=https://your-domain.com,https://www.your-domain.com

# Configurație logging
LOG_LEVEL=info
LOG_FILE=/var/log/time-management/app.log

# Configurație rate limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100
